{
    "name": "vntrungld/prometheus-exporter",
    "description": "Prometheus Exporter for Laravel",
    "license": "license",
    "authors": [
        {
            "name": "Vntrungld",
            "email": "vn.trungld@gmail.com",
            "homepage": "https://github.com/vntrungld"
        }
    ],
    "homepage": "https://github.com/vntrungld/prometheus-exporter",
    "keywords": [
        "Laravel",
        "Prometheus Exporter"
    ],
    "require": {
        "illuminate/support": "~6|~7|~8|~9|~10",
        "promphp/prometheus_client_php": "^2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~4|~5|~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "Vntrungld\\PrometheusExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vntrungld\\PrometheusExporter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vntrungld\\PrometheusExporter\\PrometheusExporterServiceProvider"
            ],
            "aliases": {
                "Prometheus": "Prometheus"
            }
        }
    }
}
